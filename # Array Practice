# Array Practice

from numpy import array
from numpy import reshape

data = [5, 9, 10, 3, 2]
data = array(data)

print(type(data))
print()

data = data.reshape((data.shape[0], 1))
print('data =', data.shape)
print()

data2 = ([0,1], [6, 8], [10,45])
data2 = array(data2)

"""
for x in data2:
	print(x)
"""

x,y = data2[:2,:], data2[2:,:]

for p in x:
	print(p)
print()

for q in y:
	print(q)